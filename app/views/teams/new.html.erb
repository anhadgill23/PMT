<%= form_for :team do |f| %>
  
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, autofocus:true, placeholder: "name"%>
  </div>
  <div class="field">
    <%= f.label :logo %><br />
    <%= f.text_field :logo, autofocus:true, placeholder: "logo"%>
  </div>

  <%= f.submit 'Create TEAM' %>

<% end %>